{
    "$schema": "https://json-schema.org/draft/2020-12/schema#",
    "$id": "qmk.definitions.v1",
    "title": "QMK Definitions",
    "description": "The qmk.json file at the root of a QMK Module",
    "version": {
        "type": "string",
        "pattern": "^[0-9]+\\.[0-9]+(\\.[0-9\\*]+)?$"
    },
    "relative_path": {
        "type": "string",
        "pattern": "^[^\\0]+$"
    }
}